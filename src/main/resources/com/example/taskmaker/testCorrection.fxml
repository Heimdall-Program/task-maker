    </TableView>
    <Label layoutX="168.0" layoutY="21.0" text="Выберите тест для редактирования">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <Button layoutX="22.0" layoutY="341.0" mnemonicParsing="false" onAction="#back" prefHeight="36.0"
            prefWidth="91.0" text="Назад"/>
    <Button layoutX="598.0" layoutY="341.0" mnemonicParsing="false" onAction="#choose"
            prefHeight="36.0" prefWidth="91.0" text="Выбрать"/>
