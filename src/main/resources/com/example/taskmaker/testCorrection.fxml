<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="725.0" stylesheets="@../../../style.css" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.taskmaker.Controllers.testCorrect">
    <TableView fx:id="table" layoutX="45.0" layoutY="73.0" prefHeight="254.0" prefWidth="636.0">
        <columns>
            <TableColumn fx:id="idtests" prefWidth="38.39996337890625" text="№"/>
            <TableColumn fx:id="name" prefWidth="261.6000289916992" text="Тема"/>
            <TableColumn fx:id="kol" prefWidth="82.39996337890625" text="Кол-во"/>
            <TableColumn fx:id="issorted" prefWidth="126.40005493164062" text="Упорядоченность"/>
            <TableColumn fx:id="category" prefWidth="126.40005493164062" text="Категория"/>
